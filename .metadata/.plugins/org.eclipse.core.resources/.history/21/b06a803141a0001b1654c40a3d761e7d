package Baekjoon;

import java.util.Scanner;

public class Main {
public static void main(String[] args) {
	String[] weeks = {"Mon", "Tue", "Wed", "Thr", "Fri", "Sat", "Sun"};
	
	for (String i: weeks) {   //type variable: literable 은 for each구문인데 더 직관적이다. 그러나 1스탭씩 순차적으로 반복되는 것만 가능
		System.out.println(i);
	
	}
}