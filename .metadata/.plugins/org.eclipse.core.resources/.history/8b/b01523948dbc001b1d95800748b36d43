package Baekjoon;

import java.util.Scanner;

public class Main {

   public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int N = sc.nextInt();
      
      int i = 1;
      int number = 2;
      
      if (N==1) {
    	  System.out.println(1);
      }else {
    	  while (N >= number + (6 * i)) {
    		  N = number + (6 * i);
    		  i++;
    	  }
    	  System.out.println(i+1);
      }
       
     
}
}
