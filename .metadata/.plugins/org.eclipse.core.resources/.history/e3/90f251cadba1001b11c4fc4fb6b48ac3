package Baekjoon;

import java.util.Scanner;

public class Main {
	public static void main( String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int i = 0;
		int NewNum = -1
				
		while (NewNum==A) {
			int x = A/10;
			int y = A%10;
			
			int c = x + y;
			
			int d = c%10;
			
			NewNum = (y*10) + d;
			i++;
		}System.out.println(i);
	}
}